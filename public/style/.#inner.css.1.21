.smile{ border: 0px solid #ff0000; text-align:center;  overflow: auto; display: block; width:98% !important; margin:3% 0; }
	
/*** ===== Sign up forms =============****/
#frmclass ul.signup{ list-style:none; border:0px solid #0FC; overflow:auto; display:block; }
#frmclass ul.signup li{ 
	float:left; border:0px solid #FF0000;
	width:47%; display:block;
	overflow:auto; min-height:225px; 
	margin:0 1%;
}
#frmclass fieldset { 
	margin: 0; 
	padding: 0;
	border:0px solid #000; 
	border-radius:5px;
} 
#frmclass fieldset label{ 
	color:#000000; min-width:44%;
	border:0px solid #000; padding:0 5px 0 0; margin:0 5px 0 0; 
	font-weight:300;
	letter-spacing:normal;
	}
#frmclass fieldset dt {
	padding:3px 0;
	} 
#frmclass fieldset dd {
	margin:0; padding:0;
	border:0px solid #33F;
	} 
#frmclass fieldset dd .min-max-char{ text-align:center; padding:0 1% 0 10%; font-size:80%; }
#frmclass fieldset input{ 
	width:53%;
} 
#frmclass fieldset select{
	width:53%; height:35px; 
	padding:7px 3px;
}
#frmclass fieldset .intrestdiv{ margin:0 3%; overflow:auto; display:block; border:0px solid #0C9;}
#frmclass fieldset .intrestdiv h3{ background:#CCC; padding:5px; margin:.5% 0;  }

.intrestdiv .maintitle { 
	overflow:auto; display:block; border:0px solid #ff0000;
	color:#414141; padding:.5% 1%; font-size:130%;
	border-top-right-radius:10px; 
	border-bottom-right-radius:10px;
	margin:1% 0;
	background: #d4e278;
	background-image: -webkit-gradient(linear, left top, right bottom, color-stop(0%, #d4e278), color-stop(100%, #a7df73));
	background-image: -webkit-linear-gradient(top left, #d4e278, #a7df73);
	background-image: -moz-linear-gradient(top left, #d4e278, #a7df73);
	background-image: -ms-linear-gradient(top left, #d4e278, #a7df73);
	background-image: -o-linear-gradient(top left, #d4e278, #a7df73);
	background-image: linear-gradient(top left, #d4e278, #a7df73);
	-webkit-box-shadow: 0 0 2px #efefef;
	-moz-box-shadow: 0 0 2px #efefef;
	box-shadow: 0 0 2px #efefef;
}

#frmclass fieldset input[type=checkbox] { padding:0; margin:0; width:4%;  margin-left:2%; background:none; border:none; }

#frmclass fieldset input[type=submit], input[type=button]{
	/*font-weight:300;*/
	padding:8px; 
	margin:1% 0;
	width:10%;
}
/***** ================ *****/
#right-content .area ul.icons{ margin: 0; padding: 0; }
#right-content .area ul.icons li{ float: left; padding: 1%; text-align: center; }

/***** ========= response ============= *******/
.response-main { border: 0px solid #ff0000; overflow: auto; display: block; padding: 1% 0;}

.last-respose { border: 3px solid #efefef; border-radius:10px; width: 20%; float: left; text-align: center; padding: 1% 0; min-height: 120px;}
.response-main .last-respose { border: 3px solid #efefef; border-radius:10px; width: 20%; float: left; text-align: center; padding: 1% 0; min-height: 120px;}
.response-main .overall-respose { border: 3px solid #efefef; border-radius:10px; width:50%; float:right; text-align: center; padding: 1% 0; min-height: 120px; }
.response-main .overall-respose .each-response {border:0px solid #ff0000; overflow: auto; display: block; width:15%; float:left; padding:.5% 2%; text-align:center;}

#responses-div { border-bottom:1px dashed #414141; overflow: auto; padding:5px 0;}
#responses-div ul{ margin:0; padding:0; border:0px solid #ff0000; display: block; overflow: auto; }
#responses-div li.response-text{ border:0px solid #414141; float:left; width:88%; padding:5px;}
#responses-div li.response-icons{ border:0px solid #414141; float:right; padding:5px; }

/***** ========= topics ============= *******/
.place-vote {background:url(../images/placevotebg.png) no-repeat 8px; padding:1.8% 0 2% 5%; border:0px solid #ff0000; color: #414141; font-size: 15px; font-weight: bold; }
.topics-main { border: 0px solid #ff0000; overflow: auto; display: block;}
.topics-main .report-v { border:0px solid #efefef; width:30%; float: left; padding-top:.5%;   }
.topics-main .report-v input.report-btn{
	width:60%; 
	font-size:90%; 
	padding:4px; 
	margin:1% 0; }
.topics-main .skip { border:0px solid #efefef; width:20%; float:right; }
.topics-main .skip  input.report-btn{
	width:70%; 
	padding:8px; 
	margin:1% 0;
}
.topics-main .overall-respose { border: 3px solid #efefef; border-radius:10px; width:50%; float:right; text-align: center; padding: 1% 0; min-height: 120px; }
.topics-main .overall-respose .each-response {border:0px solid #ff0000; overflow: auto; display: block; width:15%; float:left; padding:.5% 2%; text-align:center;}

.topic-title { border:0px solid #efefef; width:98%; padding: 1% 0; font-weight: bold; font-size:160%; line-height:150%; } 
.topic-details { border: 0px solid #efefef; width:98%; text-align: center; padding:1% 0; overflow: auto; }
.topic-details ul{ padding: 0; margin: 0; }
.topic-details li{ padding: 0; margin: 0; float: left; margin:0 3%; padding: 1%; }

/*** ===== Profile page  =============****/
#profilepage ul.profiledit{ list-style:none; border:0px solid #0FC; overflow:auto; display:block; }
#profilepage ul.profiledit li{ 
	float:left; border:0px solid #FF0000;
	width:47%; display:block;
	overflow:auto; min-height:225px;
	margin:0 1%;
}
#profilepage fieldset { 
	margin: 0; 
	padding: 0;
	border:0px solid #000; 
	border-radius:5px;
} 
#profilepage fieldset label{ 
	color:#000000; width:40%;
	border:0px solid #000; padding:0 5px 0 0; margin:0 5px 0 0; 
	font-weight:300; color:#FEFC52;
	letter-spacing:normal;
	}
#profilepage fieldset dt {
	padding:3px 0;
	} 
#profilepage fieldset dd {
	margin:0; padding:0;
	border:0px solid #FEFC52;
	min-height: 40px;
	} 
#profilepage fieldset dd .min-max-char{ text-align:center; padding:0 1% 0 10%; font-size:80%; }
#profilepage fieldset input{ 
	width:58%; 
} 
#profilepage fieldset select{
	width:58%; 
	height:35px; 
	padding:7px 3px;
}
#profilepage fieldset .intrestdiv{ margin:0 3%; overflow:auto; display:block; border:0px solid #0C9;}
#profilepage fieldset .intrestdiv h3{ background:#CCC; padding:5px; margin:.5% 0;  }

#profilepage fieldset input[type=checkbox] { padding:0; margin:0; width:4%;  margin-left:2%; background:none; border:none; }

#profilepage fieldset input[type=submit], input[type=button]{
	width:12%; 
}

/***** ================ *****/
/***** ========Invite Frinds======== *****/
#invite-frds label {
	font-size: 15px;
	font-weight: 300;
	width:20%;
	border:0px solid #0000ff;
		
}
#invite-frds input[type=text] {
	width:61%;
	padding:10px !important;
/*  width: 250px;*/
	color: #525252;
	border: 0;
	border-radius: 5px;
}

#invite-frds textarea{
	width:60%; 
	padding:7px 3px; 
}
/***** ========Invite Frinds======== *****/

/***** ======== Catcha Code ======== *****/

.catcha-div { border:3px double #C2C2C2; overflow:auto; display:block; padding:2% 0; border-radius:10px; margin:1% 8%;    }
.catcha-div ul.catchacode { border:0px solid #0000ff; overflow:auto; display:block; width:95%;}
.catcha-div ul.catchacode li { border:0px solid #FF0000; float: left;}
.catcha-div ul.catchacode li.valuetxt { border:0px solid #ff0000; width:57%; }
.catcha-div ul.catchacode li.valuetxt input{ width:30px !important;}
.catcha-div .code { width:30%; }

/***** ======== Catcha Code ======== *****/

/***** ======== top topics ======== *****/
#top-topics{ overflow: auto; display: block; border:0px solid #efefef; }
#top-topics h3{ padding:5px 0; color: #FEFC52; font-weight: bold;  }
#top-topics-new { border-bottom:0px solid #efefef; overflow: auto; }
#top-topics-new ul{ margin:0; padding:0; border:0px solid #ff0000; display: block; overflow: auto; }
#top-topics-new li.tp-text{ border:0px solid #999; float:left; width:94%; padding:5px; list-style:inside url("../images/white_arrow_right.png");}
#top-topics-new li.tp-icons{ border:0px solid #999; float:right; list-style: none;}

