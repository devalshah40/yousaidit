@charset "utf-8";
/* CSS Document */

#small-logo{ background:url(../images/ysi-logo-small.png) no-repeat center center; width:170px; height:100px; 
	border:0px solid #FFF; float:left; margin-left:1%;}
#welcome-text{ border:0px solid #FFF; float:right; width:35%; padding:4% 1% 0 1%; text-align: right; }
#separator{ background:url(../images/line-separator.png) repeat-x; height:33px;}

.error_msg { border: 1px dashed #FF0000; color:#FF0000; background: #FAFFBD; padding:1%; margin-bottom: 1%;}

#left-menucontrol { border:0px solid #FFF; float:left; width:17%; /* width:20%;*/ margin:.5%; padding:0 5px;}
#right-content { background:url(../images/content-bg.png) repeat left top; border:2px solid #979797; border-radius:10px; float:right; width:78%; /*width:75%;*/
	margin:1% 1% .5% 1%; min-height:350px;}
#right-content .pgheader{ background:url(../images/pgheader.png) left top repeat-x; padding:14px; border-top-right-radius:9px;
	border-top-left-radius:9px; overflow:auto; display:block; }
#right-content .pgheader h2{ font-family:Georgia; font-weight:bold; color:#525252; padding:5px; font-size:150%; border:0px solid #000;
	width:41%; float:left;  }
#right-content .area{ padding:10px 15px; overflow: auto; display: block; border:0px solid #903;}

#right-content .area .topicbox { 
	border:0px solid #903; min-width:95%; min-height:80px; overflow:auto; display:block; padding:8px 10px; color:#414141; margin:1% 0;
	font-size:15pt;
	-webkit-box-shadow: 0 0 4px #efefef;
	-moz-box-shadow: 0 0 4px #efefef;
	box-shadow: 0 0 4px #efefef;
	border-radius:5px;
	background:#f3f388; 
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #fefea4), color-stop(100%, #f3f388));
	background-image: -webkit-linear-gradient(top, #fefea4, #f3f388);
	background-image: -moz-linear-gradient(top, #fefea4, #f3f388);
	background-image: -ms-linear-gradient(top, #fefea4, #f3f388);
	background-image: -o-linear-gradient(top, #fefea4, #f3f388);
	background-image: linear-gradient(top, #fefea4, #f3f388);
	
	}
#left-menucontrol ul.menuctrl{ list-style:none; border:0px solid #03F; margin:0; padding:0;  }
#left-menucontrol li{  margin:3% 0; border-radius:13px; border:1px solid #414141; background:url(../images/menubg1.png) }

#left-menucontrol li a { padding:5px 8px 3px 45px; margin:3% 1%; width:65%; border:0px solid #ff0000; display:block; font-size:125%; color:#fefc52; font-weight:bold; } 

#left-menucontrol li a.home{ background:url(../images/menu-icons.png) no-repeat 8px 2px;}

#left-menucontrol li a.home-select{ background:url(../images/tab-select.png) no-repeat right top; color:#414141;}
	
#left-menucontrol li a.profile{ background:url(../images/menu-icons.png) no-repeat 8px -45px;}
#left-menucontrol li a.topics{ background:url(../images/menu-icons.png) no-repeat 8px -85px;}
#left-menucontrol li a.post{ background:url(../images/menu-icons.png) no-repeat 8px -128px;}
#left-menucontrol li a.responses{ background:url(../images/menu-icons.png) no-repeat 8px -128px;}
#left-menucontrol li a.settings{ background:url(../images/menu-icons.png) no-repeat 8px -175px;}
#left-menucontrol li a.logout{ background:url(../images/menu-icons.png) no-repeat 8px -218px;}
